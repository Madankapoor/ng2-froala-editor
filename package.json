{
  "name": "ng2-froala-editor",
  "version": "1.0.15",
  "description": "Froala Editor for Angular2",
  "repository": {
    "type": "git",
    "url": "https://github.com/Thrinacia/ng2-froala-editor"
  },
  "scripts": {
    "typings-install": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "play": "webpack-dev-server --inline --port 8085",
    "build-cjs": "tsc -p tsconfig.common.json",
    "build-system": "tsc -p tsconfig.system.json",
    "build": "rimraf dist && npm run build-cjs && npm run build-system && webpack --progress --colors --profile --bail --config webpack.prod.js && gulp build",
    "test": "karma start config/karma.conf.js"
  },
  "license": "ISC",
  "devDependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "angular2-template-loader": "^0.4.0",
    "concurrently": "^2.2.0",
    "core-js": "^2.4.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "gulp": "^3.9.1",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "lite-server": "^2.2.2",
    "phantomjs-prebuilt": "^2.1.13",
    "protractor": "^4.0.9",
    "reflect-metadata": "^0.1.8",
    "rxjs": "^5.0.0-beta.12",
    "systemjs": "^0.19.39",
    "ts-loader": "^0.8.2",
    "typescript": "^2.0.3",
    "typings": "^1.4.0",
    "webpack": "2.1.0-beta.22",
    "webpack-dev-server": "2.1.0-beta.9",
    "webpack-merge": "^0.14.1",
    "zone.js": "^0.6.25"
  }
